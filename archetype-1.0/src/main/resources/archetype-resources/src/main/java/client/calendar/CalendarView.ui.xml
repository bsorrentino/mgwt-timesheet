#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style field='cal' src="CalendarView.css"/>    

	<g:HTMLPanel>
		<div class="{cal.holder}">
	    	<div class="{cal.month} {cal.yellow}">
	        	<p><g:Label ui:field="lblMonthName">month</g:Label></p>
	    </div>
	    <div class="{cal.day}">
	        <p><g:Label ui:field="lblDayOrdinal">day</g:Label></p>
	    </div>
		</div>	
	</g:HTMLPanel>
</ui:UiBinder> 