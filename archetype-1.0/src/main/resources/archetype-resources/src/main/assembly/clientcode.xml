#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<assembly>
  <id>clientcode</id>
  <includeBaseDirectory>false</includeBaseDirectory>
  <formats>
    <format>zip</format>
  </formats>
  <fileSets>
        <fileSet>
                <directory>target/${symbol_dollar}{project.artifactId}-${symbol_dollar}{project.version}</directory>
                <includes>
                        <include>**/*.js</include>
                        <include>**/*.png</include>
                        <include>**/*.rpc</include>
                        <include>**/*.html</include>
                        <include>**/*.gif</include>
                </includes>
                <outputDirectory></outputDirectory>
        </fileSet>
  </fileSets>
</assembly>